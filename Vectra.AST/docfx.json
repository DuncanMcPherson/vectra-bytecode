{
  "build": {
    "content": [
      {
        "files": [ "articles/**/*.md" ],
        "dest": "."
      },
      {
        "files": [ "toc.yml" ]
      }
    ],
    "resource": [],
    "overwrite": [],
    "dest": "docs/wiki"
  },
  "metadata": [
    {
      "src": [
        {
          "files": [ "Vectra.AST.csproj"],
          "exclude": ["**/bin/**", "**/obj/**"]
        }
      ],
      "dest": "articles",
      "outputFormat": "markdown"
    }
  ],
  "tocGeneration": {
    "autoGenerateToc": true
  }
}
