{
  "metadata": [
    {
      "src": [
        {
          "files": [ "Vectra.AST.csproj"],
          "exclude": ["**/bin/**", "**/obj/**"]
        }
      ],
      "dest": "articles",
      "outputFormat": "markdown"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["articles/**/*.md"]
      }
    ],
    "dest": "_site"
  },
  "tocGeneration": {
    "autoGenerateToc": true
  }
}
